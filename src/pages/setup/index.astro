---
import SetupWizard from "@/domain/auth/components/setup-wizard";
import { Setup } from "@/domain/auth/models/user";
import BaseLayout from "@/layouts/BaseLayout.astro";

// Redirect to dashboard if setup is already complete
if (Setup.isComplete()) {
	return Astro.redirect("/dashboard");
}
---


<BaseLayout>
    <SetupWizard client:load />
</BaseLayout>
